int a=5;
			int b=0;
			try {
			int c= a/b;
			System.out.println("Result is:" +c);
			}
			catch (ArithmeticException e) {
				System.out.println("Error: division is not allowed");
				
			}
			finally {
				System.out.println("Execution complete");
		}
	}

System.out.println("Execution is pass or fail:" +c);
}
